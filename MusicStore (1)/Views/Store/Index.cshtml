@model IEnumerable<MusicStore__1_.Models.Genre>

@{
    ViewData["Title"] = "Index";
}

<h1>Browse Genres</h1>

<p>Select from @Model.Count()</p>
<ul>
    @foreach (var genre in Model)
    {
        <li>
            @Html.ActionLink(genre.Name,
        "Browse", new { genre = genre.Name })
        </li>
    }
</ul>